{
  "name": "ia-vscode",
  "displayName": "ia-vscode",
  "description": "ItemsAdder vscode extension",
  "version": "0.2.13",
  "publisher": "LoneDev",
  "icon": "images/ia-logo.png",
  "bugs": {
    "url": "https://github.com/ItemsAdder/ia-vscode/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ItemsAdder/ia-vscode.git"
  },
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/unzipper": "^0.10.11",
    "@types/vscode": "^1.67.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2"
  },
  "dependencies": {
    "java-parser": "^2.3.3",
    "unzipper": "^0.12.3",
    "yaml": "^2.7.0",
    "typescript": "^4.9.5"
  },
  "activationEvents": [
    "onLanguage:yaml"
  ],
  "extensionDependencies": [
    "redhat.vscode-yaml"
  ],
  "contributes": {
    "configuration": {
      "title": "IA VS Code Extension",
      "properties": {
        "ia-vscode.neverWarnAboutCopilot": {
          "type": "boolean",
          "default": false,
          "description": "Disables warnings about GitHub Copilot in this extension."
        },
        "ia-vscode.devMode": {
          "type": "boolean",
          "default": false,
          "description": "Enables dev mode for the extension."
        }
      }
    },
    "languages": [
      {
        "id": "jspp",
        "aliases": [
          "jspp"
        ],
        "extensions": [
          ".jspp"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "jspp",
        "scopeName": "source.jspp",
        "path": "./syntaxes/jspp.tmLanguage.json"
      }
    ]
  }
}
